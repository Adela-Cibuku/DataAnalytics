    "# Explode categories (if they are stored as a list)\n",
    "df_pandas[\"categories\"] = df_pandas[\"categories\"].astype(str)\n",
    "df_categories = df_pandas.assign(category=df_pandas[\"categories\"].str.split(\", \")).explode(\"category\")\n",
    "\n",
    "# Average stars per category\n",
    "category_sentiment = df_categories.groupby(\"category\")[\"stars\"].mean().sort_values()\n",
    "\n",
    "# Plot sentiment per category\n",
    "plt.figure(figsize=(10, 6))\n",
    "category_sentiment.tail(10).plot(kind=\"barh\", color=\"green\")\n",
    "plt.title(\"Top 10 Categories with Highest Ratings\")\n",
    "plt.xlabel(\"Average Stars\")\n",
    "plt.show()\n"
