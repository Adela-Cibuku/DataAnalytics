   "source": [
    "# If not already defined, load the dataset and convert to Pandas\n",
    "from pyspark.sql import SparkSession\n",
    "import pandas as pd\n",
    "\n",
    "# Initialize Spark session\n",
    "spark = SparkSession.builder.appName(\"YelpDataAnalysis\").getOrCreate()\n",
    "\n",
    "# Load dataset\n",
    "df = spark.read.json(\"DA_Project.json\")\n",
    "\n",
    "# Convert to Pandas DataFrame\n",
    "df_pandas = df.toPandas()\n",
    "\n",
    "# Enhance Correlation Analysis with More Attributes\n",
    "num_cols = [\"stars\", \"review_count\", \"is_open\", \"latitude\", \"longitude\"]\n",
    "corr_matrix = df_pandas[num_cols].corr()\n",
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "plt.figure(figsize=(8, 5))\n",
    "sns.heatmap(corr_matrix, annot=True, cmap=\"coolwarm\")\n",
    "plt.title(\"Enhanced Correlation Heatmap with Additional Attributes\")\n",
    "plt.show()\n",
    "\n"
   ]
  },
